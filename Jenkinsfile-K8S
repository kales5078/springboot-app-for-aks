pipeline {
    agent any
    stages {
         stage ("Kube Deploy") {
            steps {
                withKubeConfig(clusterName: 'POC', credentialsId: '8799cbb5-0db1-405c-a1ee-5a2635fb1bbd', namespace: 'default',) {
                 sh "kubectl apply -f eks-deploy-from-ecr.yaml"
                }
            }
        }
  }
}
