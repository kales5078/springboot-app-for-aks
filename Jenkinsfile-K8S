pipeline {
    agent any

    environment {
        registry = "817331706303.dkr.ecr.us-east-1.amazonaws.com/nuget-repo:latest"
    }
    
    stages {         
        stage ("Kube Deploy") {
            steps {
                withKubeConfig(caCertificate: '', clusterName: 'POC', credentialsId: '8799cbb5-0db1-405c-a1ee-5a2635fb1bbd', namespace: 'default') {
                 sh "kubectl apply -f eks-deploy-from-ecr.yaml"
                }
            }
        }
    }
}
